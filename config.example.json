{
    "app": {
        "environment": "<environment>",
        "dataDirectory": "/home/captbunzo/data",
        "collectorTimeoutSecs": 60,
        "messagePurgeDelaySecs": 75,
        "schedulerLoopSecs": 60
    },

    "battles": {
        "showMemberTrainerNames": true,
        "blockHostSelfJoin": false
    },

    "log": {
        "level": "debug",
        "sql": false
    },

    "discord": {
        "client_id": "<discord-application-id>",
        "token": "<bot-token>",
        "client_secret": "<client-secret>",
        "botGuildId": "<bot-guild-id>"
    },

    "database": {
        "client": "mysql",
        "host": "127.0.0.1",
        "port": 3306,
        "name": "rotom",
        "user": "rotom",
        "password": "<password>",
        "collation": "utf8mb4_unicode_ci"
    },

    "about_botAdminUserIds_config": "Bot Admins role ID string array",
    "botAdminUserIds": ["<user-id>", "<another-user-id>"],

    "sentry": {
        "dsn": "<sentry-dsn>",
        "tracesSampleRate": 1.0,
        "triggerSampleAlerts": false,
        "release": "rotom-bot@1.00.00"
    },

    "about_intents_config": "Intents the bot needs - this is not yet used",
    "intents": [
        "GUILD_MEMBERS",
        "GUILD_MESSAGES",
        "GUILD_MESSAGE_REACTIONS",
        "DIRECT_MESSAGES",
        "DIRECT_MESSAGE_REACTIONS"
    ]
}
